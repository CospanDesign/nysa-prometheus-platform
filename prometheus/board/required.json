{
  "BASE_DIR":"~/projects/ibuilder_project",
  "board":"prometheus",
  "PROJECT_NAME":"prometheus_default",
  "TEMPLATE":"wishbone_template.json",
  "INTERFACE":{
    "filename":"ppfifo_host_interface.v"
  },
  "SLAVES":{
    "prometheus":{
      "filename":"wb_prometheus.v",
      "bind":{
        "o_gpif_clk":{
          "loc":"o_gpif_clk",
          "direction":"output"
        },
        "o_cs_n":{
          "loc":"o_gpif_cs_n",
          "direction":"output"
        },
        "o_oe_n":{
          "loc":"o_gpif_oe_n",
          "direction":"output"
        },
        "o_we_n":{
          "loc":"o_gpif_we_n",
          "direction":"output"
        },
        "o_re_n":{
          "loc":"o_gpif_re_n",
          "direction":"output"
        },
        "o_pkt_end_n":{
          "loc":"o_gpif_pkt_end_n",
          "direction":"output"
        },
        "io_data":{
          "loc":"io_gpif_data[31:0]",
          "direction":"inout"
        },
        "i_in_rdy":{
          "loc":"i_gpif_in_rdy",
          "direction":"input"
        },
        "i_out_rdy":{
          "loc":"i_gpif_out_rdy",
          "direction":"input"
        },
        "o_socket_addr":{
          "loc":"o_gpif_socket_addr[1:0]",
          "direction":"output"
        },
        "o_gpif_int_n":{
          "loc":"o_gpif_interrupt_n",
          "direction":"output"
        }
      }
    },
    "gpio1":{
      "filename":"wb_gpio.v",
      "bind":{
        "gpio_out[3:0]":{
          "loc":"o_led_n[3:0]",
          "direction":"output"
        },
        "gpio_in[7:4]":{
          "loc":"i_button[3:0]",
          "direction":"input"
        },
        "gpio_in[9:8]":{
          "loc":"i_switch[1:0]",
          "direction":"input"
        }
      }
    }
  },
  "board_bind":{
    "rst":{
      "direction":"input",
      "loc":"rst"
    },
    "clk":{
      "direction":"input",
      "loc":"clk"
    }
  },
  "board_internal_bind":{
    "i_ingress_rdy":{
      "signal":"prometheus_o_ingress_ready"
    },
    "prometheus_i_ingress_activate":{
      "signal":"o_ingress_act"
    },
    "i_ingress_size":{
      "signal":"prometheus_o_ingress_packet_size"
    },
    "i_ingress_data":{
      "signal":"prometheus_o_ingress_data"
    },
    "prometheus_i_ingress_strobe":{
      "signal":"o_ingress_stb"
    },
    "i_egress_rdy":{
      "signal":"prometheus_o_egress_ready"
    },
    "prometheus_i_egress_activate":{
      "signal":"o_egress_act"
    },
    "i_egress_size":{
      "signal":"prometheus_o_egress_size"
    },
    "prometheus_i_egress_data":{
      "signal":"o_egress_data"
    },
    "prometheus_i_egress_strobe":{
      "signal":"o_egress_stb"
    }
  }
}
